{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "no-protocol-relative-urls": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not ie <= 11",
    "not opera <= 99",
    "not samsung <= 21"
  ]
}